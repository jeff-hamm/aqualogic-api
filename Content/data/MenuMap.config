{
    "None": [
        {
            "name": "Date",
            "test": "^(?<Day>Sunday|Monday|Tuesday|Wednesday|Thursday|Friday|Saturday)\n(?<Hour>\d+).(?<Minute>\d+)(?<Meridiem>[AP])$",
            "value": "$1 $2:$3 $4M",
            "unit": "DateTime",
            "type": "Status"
        },
        {
            "name": "Pool Temp",
            "test": "^Pool Temp\n(?<Value>[\d\.]+)_(?<Unit>[FfCc])$",
            "value": "$1°",
            "unit": "Degrees",
            "type": "Status"
        },
        {
            "name": "Spa Temp",
            "test": "^Spa Temp\n(?<Value>[\d\.]+)_(?<Unit>[FfCc])$",
            "value": "$1°",
            "unit": "Degrees",
            "type": "Status"
        },
        {
            "name": "Air Temp",
            "test": "^Air Temp\n(?<Value>[\d\.]+)_(?<Unit>[FfCc])$",
            "value": "$1°",
            "unit": "Degrees",
            "type": "Status"
        },
        {
            "name": "Pool Chlorinator",
            "test": "^Pool Chlorinator\n(?<Value>[\d\.]+)%$",
            "value": "$1%",
            "unit": "Percentage",
            "type": "Status"
        },
        {
            "name": "Spa Chlorinator",
            "test": "^Spa Chlorinator\n(?<Value>[\d\.]+)%$",
            "value": "$1%",
            "unit": "Percentage",
            "type": "Status"
        },
        {
            "name": "Salt Level",
            "test": "^Salt Level\n(?<Value>[\d\.]+)\s+(?<Unit>g/L|PPM|)$",
            "value": "$1 $2",
            "unit": "Concentration",
            "type": "Status"
        },
        {
            "name": "Check System",
            "test": "^Check System\n(?<Value>.+)$",
            "value": "$1",
            "unit": "String",
            "type": "Status"
        },
        {
            "name": "Heater1",
            "test": "^Heater1\n(?<Value>Auto Control|Manual Off)$",
            "value": "$1",
            "unit": "AutoMode",
            "type": "Status"
        }
    ],
    "menu": [
        {
            "name": "Settings",
            "test": "^Settings\nMenu$",
            "type": "Menu",
            "inputs": "Left,Right",
            "children": [
                {
                    "name": "Spa Heater1",
                    "test": "^Spa Heater1\n((?<Value>[\d\.]+)_(?<Unit>[FfCc])|(?<Value>Off))$",
                    "value": "${Value} ${Unit}",
                    "unit": "Degrees",
                    "type": "Setting",
                    "inputs": "Up,Down"
                },
                {
                    "name": "Pool Heater1",
                    "test": "^Pool Heater1\n((?<Value>[\d\.]+)_(?<Unit>[FfCc])|(?<Value>Off))$",
                    "value": "${Value} ${Unit}",
                    "unit": "Degrees",
                    "type": "Setting",
                    "inputs": "Up,Down"
                },
                {
                    "name": "Spa Solar",
                    "test": "^Spa Solar\n(?<Value>[\d\.]+)_(?<Unit>[FfCc])$",
                    "value": "$1°",
                    "unit": "Degrees",
                    "type": "Setting",
                    "inputs": "Up,Down"
                },
                {
                    "name": "Pool Solar",
                    "test": "^Pool Solar\n(?<Value>[\d\.]+)_(?<Unit>[FfCc])$",
                    "value": "$1°",
                    "unit": "Degrees",
                    "type": "Setting",
                    "inputs": "Up,Down"
                },
                {
                    "name": "Super Chlorinate",
                    "test": "^Super Chlorinate\n(?<Value>Off|On))$",
                    "value": "$1",
                    "unit": "Toggle",
                    "type": "Setting",
                    "inputs": "Up,Down"
                },
                {
                    "name": "Spa Chlorinator",
                    "test": "^Spa Chlorinator\n(?<Value>[\d\.]+)%$",
                    "value": "$1%",
                    "unit": "Percentage",
                    "type": "Setting",
                    "inputs": "Up,Down"
                },
                {
                    "name": "Pool Chlorinator",
                    "test": "^Pool Chlorinator\n(?<Value>[\d\.]+)%$",
                    "value": "$1%",
                    "unit": "Percentage",
                    "type": "Setting",
                    "inputs": "Up,Down"
                },
                {
                    "name": "Set Day and Time - Day",
                    "test": "^Set Day and Time\n(?<Day>[^\s]+)\s+(?<Hour>\d+).(?<Minute>\d+)(?<Meridiem>[AP])$",
                    "value": "${Day}",
                    "unit": "Day",
                    "type": "Setting",
                },
                {
                    "name": "Set Day and Time - Hour",
                    "test": "^Set Day and Time\n(?<Day>[^\s]+)\s+(?<Hour>\d+).(?<Minute>\d+)(?<Meridiem>[AP])$",
                    "value": "${Hour}:${Minute} ${Meridiem}M",
                    "unit": "Time",
                    "type": "Setting",
                },
                {
                    "name": "Set Day and Time - Minute",
                    "test": "^Set Day and Time\n(?<Day>[^\s]+)\s+(?<Hour>\d+).(?<Minute>\d+)(?<Meridiem>[AP])$",
                    "value": "${Hour}:${Minute} ${Meridiem}M",
                    "unit": "Time",
                    "type": "Setting",
                },
                {
                    "name": "Backlight on/off:",
                    "test": "^Backlight on/off:\n(?<Value>.+)$",
                    "unit": "String",
                    "type": "Ignore",
                },
                {
                    "name": "Beeper",
                    "test": "^Beeper\n(?<Value>.+)$",
                    "unit": "String",
                    "type": "Ignore",
                },
                {
                    "name": "Teach Wireless",
                    "test": "^Teach Wireless:\n(?<Value>.+)$",
                    "unit": "String",
                    "type": "Ignore",
                    "inputs": "Up"
                },
                {
                    "name": "Wireless",
                    "test": "^Wireless:\nChannel: (?<Value>\d+)$",
                    "unit": "Integer",
                    "type": "Setting",
                    "inputs": "Up,Down"
                },
            ]
        },
        {
            "name": "Timers",
            "test": "^Timers\nMenu$",
            "type": "Menu",
            "children": []
        },
        {
            "name": "Diagnostic",
            "test": "^Diagnostic\nMenu$",
            "type": "Menu",
            "children": []
        },
        {
            "name": "Configuration",
            "test": "^Configuration\nMenu(?<Locked>-Locked)?$",
            "value": "$1",
            "type": "Menu",
            "children": []
        },
        {
            "name": "Default",
            "test": "^Default\nMenu?$",
            "type": "Menu",
            "children": []
        },
    ]
